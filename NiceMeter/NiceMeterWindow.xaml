        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d">
    <StackPanel Orientation="Horizontal">
        <StackPanel x:Name="mainboardPanel" Orientation="Horizontal"></StackPanel>
        <StackPanel x:Name="ramPanel" Orientation="Horizontal"></StackPanel>
        <StackPanel x:Name="hddPanel" Orientation="Horizontal"></StackPanel>
        <StackPanel x:Name="cpuPanel" Orientation="Horizontal"></StackPanel>
    </StackPanel>
</Window>
